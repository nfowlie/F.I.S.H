    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-armv7l": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=armv7l --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "package-arm64": "electron-packager . fish --overwrite --asar=true --platform=linux --arch=arm64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds"
